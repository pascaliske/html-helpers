{
  "name": "@pascaliske/html-helpers",
  "description": "Collection of helper functions for HTML.",
  "version": "1.5.7",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/html-helpers.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/html-helpers/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "browser": "dist/index.umd.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rollup --config rollup.config.ts",
    "watch": "rollup --config rollup.config.ts --watch",
    "docs": "typedoc --mode modules --out docs/ --excludeNotExported --excludeExternals src/styles.ts",
    "lint": "tslint --project tsconfig.json --fix",
    "test": "jest --config jest.config.json --coverage && codecov",
    "check": "snyk test",
    "format": "prettier --write \"src/**/*.ts\"",
    "changelog": "standard-changelog --release-count 1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@pascaliske/tsconfig": "^1.0.4",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.16",
    "codecov": "^3.5.0",
    "expect": "^24.8.0",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "rollup": "^1.17.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript2": "^0.22.1",
    "snyk": "^1.207.0",
    "standard-changelog": "^2.0.13",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.3"
  }
}

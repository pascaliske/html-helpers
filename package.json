{
  "name": "@pascaliske/html-helpers",
  "description": "Collection of helper functions for HTML.",
  "version": "1.2.2",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/html-helpers.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/html-helpers/issues"
  },
  "main": "dist/index.js",
  "typings": "typings/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "watch": "nodemon --exec \"rm -rf dist typings && yarn run build\" --watch src --ext ts",
    "lint": "tslint --project tsconfig.json --fix",
    "test": "ava tests/**/*.test.js",
    "check": "snyk test",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "classnames": "^2.2.6"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "ava": "^1.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.3",
    "snyk": "^1.124.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.3.1"
  }
}

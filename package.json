{
  "name": "@pascaliske/html-helpers",
  "description": "Collection of helper functions for HTML.",
  "version": "1.5.7",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/html-helpers.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/html-helpers/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "browser": "dist/index.umd.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rollup --config rollup.config.ts",
    "watch": "rollup --config rollup.config.ts --watch",
    "docs": "typedoc --mode modules --out docs/ --excludeNotExported --excludeExternals src/styles.ts",
    "lint": "tslint --project tsconfig.json --fix",
    "test": "jest --config jest.config.json --coverage && codecov",
    "check": "snyk test",
    "format": "prettier --write \"src/**/*.ts\"",
    "version": "bump --yes package.json",
    "changelog": "standard-changelog"
  },
  "prettier": "@pascaliske/prettier-config",
  "dependencies": {
    "classnames": "^2.2.6",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@pascaliske/prettier-config": "^1.0.2",
    "@pascaliske/tsconfig": "^1.0.4",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.9.0",
    "bump-cli": "^1.1.3",
    "codecov": "^3.6.1",
    "expect": "^24.9.0",
    "husky": "^4.0.10",
    "jest": "^24.9.0",
    "lint-staged": "^10.0.1",
    "prettier": "^1.19.1",
    "rollup": "^1.29.1",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "snyk": "^1.279.0",
    "standard-changelog": "^2.0.21",
    "ts-jest": "^24.3.0",
    "tslint": "^6.0.0-beta1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.16.8",
    "typescript": "^3.7.5"
  }
}

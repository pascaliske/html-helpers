{
  "name": "@pascaliske/html-helpers",
  "description": "Collection of helper functions for HTML.",
  "version": "1.5.8",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/html-helpers.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/html-helpers/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "browser": "dist/index.umd.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rollup --config rollup.config.ts",
    "watch": "rollup --config rollup.config.ts --watch",
    "docs": "typedoc --mode modules --out docs/ --excludeNotExported --excludeExternals src/styles.ts",
    "lint": "tslint --project tsconfig.json --fix",
    "test": "jest --config jest.config.json --coverage && codecov",
    "format": "prettier --write \"src/**/*.ts\"",
    "version": "bump --yes package.json",
    "changelog": "standard-changelog"
  },
  "prettier": "@pascaliske/prettier-config",
  "dependencies": {
    "classnames": "^2.2.6",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@pascaliske/prettier-config": "^1.1.0",
    "@pascaliske/tsconfig": "^1.0.4",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.19",
    "bump-cli": "^1.1.3",
    "codecov": "^3.8.1",
    "expect": "^26.6.2",
    "husky": "^4.3.6",
    "jest": "^25.5.4",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "rollup": "^2.36.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "standard-changelog": "^2.0.27",
    "ts-jest": "^25.5.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.19.2",
    "typescript": "^3.9.7"
  }
}
